{
    "name": "dinkydodouk/whm",
    "description": "A link to the Dodo WHM server",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "whm"
    ],
    "support": {
        "docs": "https://github.com/dinkydodouk/whm/blob/master/README.md",
        "issues": "https://github.com/dinkydodouk/whm/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dodo Technologies Ltd",
            "homepage": "https://www.dinkydodo.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "stripe/stripe-php": "7.30.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "dinkydodouk\\whm\\": "src/"
        }
    },
    "extra": {
        "name": "WHM",
        "handle": "whm",
        "developer": "Dodo Technologies Ltd",
        "developerUrl": "https://www.dinkydodo.com",
        "documentationUrl": "https://github.com/dinkydodouk/whm/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/dinkydodouk/whm/master/CHANGELOG.md",
        "components": {
            "WHMService": "dinkydodouk\\whm\\services\\WHMService"
        },
        "class": "dinkydodouk\\whm\\WHM"
    }
}
